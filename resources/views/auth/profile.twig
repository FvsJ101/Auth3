{% extends "templates/default_no_banner.twig" %}

{% block content %}
	{#{{ auth.userInfo | json_encode }}#}
	<!-- Page Heading/Breadcrumbs -->
<div class="container">
	<div class="row">
		<div class="col-lg-12" style="background-color: #000000">
			<div style="background-color: #000000" align="center"><img src="{{ base_url() }}/images/logo/service-logo-.png" alt="service-logo"></div>
			<h2 style="margin-bottom: 50px;" align="center">SIGN IN</h2>
		</div>
	<!-- /.row -->
	
	<!----------------breadCrumbs---------------->
	{% include 'templates/partials/breadcrumbs.twig' %}
	<!----------------/.breadCrumbs---------------->
	<!----------------flashMessage---------------->
	{% include 'templates/partials/flash.twig' %}
	<!----------------/.flashMessage---------------->
	{#{{ user | json_encode }}#}
	<div class="row">
		<div class="col-md-6">
			<div class="panel panel-default">
				<!-- Default panel contents -->
				<div class="panel-heading"><h4>Your Information</h4></div>
				<div class="panel-body" style="color: #000000;">
					<p>Type: {{ user.admin_type }}</p>
					<p>Username: {{ user.username }}</p>
					<p>Name: {{ user.first_name }} {{ user.last_name }} </p>
					<p>Email: {{ user.email }}</p>
					<p>Registered Date: {{ user.created_at }}</p>
				</div>
			</div>
		</div>
		<div class="col-md-6">
			blank
		</div>
	</div>
</div>
	

{% endblock %}
